    <!-- Messages -->
    <string name="message_help_dialog">Para que tudo funcione direitinho, por favor, conceda as permissões necessárias. Basta ativar os botões e aceitar os termos para continuar.</string>
    <string name="message">Sua mensagem aqui</string>
    <string name="message_give_permissions">Precisamos de algumas permissões para que tudo funcione bem. Por favor, conceda-as abaixo:</string>
    <string name="message_permission_denied">As permissões são necessárias para o app funcionar. Por favor, ative-as nas configurações para prosseguir.</string>
    <string name="message_overlay_permission">Precisamos dessa permissão para mostrar a prévia da câmera durante a gravação. Por favor, permita-a para usar o recurso de prévia.</string>
    <string name="message_video_not_available">Desculpe, o vídeo não pode ser reproduzido porque o arquivo não está disponível.</string>
